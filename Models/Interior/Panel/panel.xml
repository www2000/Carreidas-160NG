<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- New version by MyName
     Updated by Helijah 2013       -->
<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>
    <object-name>manette1G</object-name>
    <object-name>manette2G</object-name>
    <object-name>manette3G</object-name>
    <object-name>manette1D</object-name>
    <object-name>manette2D</object-name>
    <object-name>manette3D</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- Farmin pannel -->
  <model>
    <path>Aircraft/Carreidas-160NG/Models/Interior/Panel/Instruments/Farmin/GDU-1040.screen1.xml</path>
    <offsets>
        <x-m>  0.741 </x-m>
        <y-m> -0.500 </y-m>
        <z-m>  0.000 </z-m>
    </offsets>
  </model>
  <model>
      <path>Aircraft/Carreidas-160NG/Models/Interior/Panel/Instruments/Farmin/GDU-1040.screen2.xml</path>
    <offsets>
        <x-m>  0.741 </x-m>
        <y-m>  0.000 </y-m>
        <z-m>  0.000 </z-m>
    </offsets>
  </model>
  <model>
      <path>Aircraft/Carreidas-160NG/Models/Interior/Panel/Instruments/Farmin/GDU-1040.screen3.xml</path>
    <offsets>
        <x-m>  0.741 </x-m>
        <y-m>  0.500 </y-m>
        <z-m>  0.000 </z-m>
    </offsets>
  </model>

  <!-- OverHead -->
  <model>
    <path>Aircraft/Carreidas-160NG/Models/Interior/Panel/overhead/overhead.xml</path>
    <offsets>
      <x-m> 0.800 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.608 </z-m>
      <pitch-deg> 69.31 </pitch-deg>
    </offsets>
  </model>

  <!-- Reverser toggle -->
  <model>
    <path>Aircraft/Carreidas-160NG/Models/Interior/Panel/Instruments/reverser/reverser.xml</path>
    <offsets>
      <x-m>  1.163 </x-m>
      <y-m> -0.134 </y-m>
      <z-m> -0.356 </z-m>
      <pitch-deg> -90 </pitch-deg>
    </offsets>
  </model>

  <model>
    <path>Aircraft/Carreidas-160NG/Models/Interior/Panel/Instruments/reverser/reverser.xml</path>
    <offsets>
      <x-m>  1.163 </x-m>
      <y-m>  0.134 </y-m>
      <z-m> -0.356 </z-m>
      <pitch-deg> -90 </pitch-deg>
    </offsets>
  </model>

  <!-- Throttle -->
  <animation>
    <type>translate</type>
    <object-name>manette1G</object-name>
    <object-name>manette1D</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 0.000 </dep></entry>
      <entry><ind> 1 </ind><dep> 0.155 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>manette2G</object-name>
    <object-name>manette2D</object-name>
    <property>controls/engines/engine[1]/throttle</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 0.000 </dep></entry>
      <entry><ind> 1 </ind><dep> 0.155 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>manette3G</object-name>
    <object-name>manette3D</object-name>
    <property>controls/engines/engine[2]/throttle</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 0.000 </dep></entry>
      <entry><ind> 1 </ind><dep> 0.155 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
